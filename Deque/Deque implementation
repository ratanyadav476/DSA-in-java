import java.util.Deque;
import java.util.LinkedList;

/**
 * DequeImplementation
 */
public class DequeImplementation {

    public static void main(String[] args) {
        Deque<Integer> deque = new LinkedList<>();
        deque.addFirst(1);
        deque.addLast(2);
        deque.addLast(3);
        deque.addLast(4);
        deque.removeFirst();
        deque.removeFirst();
        deque.removeLast();
        System.out.println(deque);

    }
}
